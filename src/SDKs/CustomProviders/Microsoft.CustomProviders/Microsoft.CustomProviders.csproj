<Project Sdk="Microsoft.NET.Sdk">
	<!-- Please do not move/edit code below this line -->
	<Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
	<!-- Please do not move/edit code below this line -->

	<PropertyGroup>
		<Description>Provides developers with libraries to create and manage custom provider resource. Note: This client library is for CustomProviders under Azure Resource Manager.</Description>
		<Version>0.1.1-preview</Version>
		<AssemblyName>Microsoft.CustomProviders</AssemblyName>
		<PackageId>Microsoft.CustomProviders</PackageId>
		<PackageTags>CustomProviders;</PackageTags>
		<PackageReleaseNotes />
    <GenerateResourceNeverLockTypeAssemblies>true</GenerateResourceNeverLockTypeAssemblies>
	</PropertyGroup>
	<PropertyGroup>
		<TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
	</PropertyGroup>
	<ItemGroup>
	  <Folder Include="Generated\" />
	</ItemGroup>

	<!-- Please do not move/edit code below this line -->
	<Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
	<!-- Please do not move/edit code above this line -->
</Project>

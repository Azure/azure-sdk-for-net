<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>CodeGenerationLibrary</PackageId>
    <Description>Azure Batch object model code generation library</Description>
    <!--After fixing all warnings, enable this option-->
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <AssemblyName>CodeGenerationLibrary</AssemblyName>
    <RestorePackagesPath>$(LibraryNugetPackageFolder)</RestorePackagesPath>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>net46</TargetFrameworks>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>

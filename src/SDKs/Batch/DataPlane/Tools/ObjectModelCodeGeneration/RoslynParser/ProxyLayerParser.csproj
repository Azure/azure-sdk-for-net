<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>ProxyLayerParser</PackageId>
    <Description>Azure Batch roslyn based parser for generating code</Description>
    <!--After fixing all warnings, enable this option-->
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <AssemblyName>ProxyLayerParser</AssemblyName>
    <RestorePackagesPath>$(LibraryNugetPackageFolder)</RestorePackagesPath>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>net46</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Build" Version="15.5.180" />
    <PackageReference Include="Microsoft.Build.Tasks.Core" Version="15.5.180" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" Version="2.6.1" />
  </ItemGroup>

</Project>

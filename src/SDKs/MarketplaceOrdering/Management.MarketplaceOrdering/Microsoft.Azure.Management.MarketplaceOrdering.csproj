<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
  <PropertyGroup>
    <PackageId>Microsoft.Azure.Management.MarketplaceOrdering</PackageId>
    <Description>Provides developers with a library to get and set legal terms for Azure Marketplace Offers. Note: This client library is for MarketplaceOrdering under Azure Resource Manager.</Description>
    <VersionPrefix>1.0.0</VersionPrefix>
    <AssemblyName>Microsoft.Azure.Management.MarketplaceOrdering</AssemblyName>
    <PackageTags>Microsoft Azure MarketplaceOrdering Management;REST HTTP client;windowsazureofficial;netcore451511</PackageTags>
	<PackageReleaseNotes>
      <![CDATA[
        1) Added Get Api for Agreements for Virtual Machine Offers 
        2) Added Create Api for Agreements for Virtual Machine Offers to allow users to accept or reject agreement terms
      ]]>
    </PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
  </PropertyGroup>
</Project>

<Project Sdk="Microsoft.NET.Sdk">
  <!-- Please do not move/edit code below this line -->
  <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
  <!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Azure.Management.IotCentral</PackageId>
    <Description>Provides management capabilities for Microsoft Azure IotCentral.</Description>
    <AssemblyTitle>Microsoft Azure IotCentral Management</AssemblyTitle>
    <AssemblyName>Microsoft.Azure.Management.IotCentral</AssemblyName>
    <Version>2.0.0</Version>
    <PackageTags>Microsoft Azure IotCentral;IotCentral management;IotCentral;</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
        This release adds a new API for checking the uniqueness of the application subdomain. 
        Fixes the expected error object returned from Microsoft.IotCentral Rp service, for friendly messages.
        Allows checking the uniqueness of different Microsoft.IotCentral resource types.
      ]]>
    </PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
  </PropertyGroup>

  <!-- Please do not move/edit code below this line -->
  <Import Condition="Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))')" Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
  <!-- Please do not move/edit code above this line -->
</Project>
<Project Sdk="Microsoft.NET.Sdk">
	<!-- Please do not move/edit code below this line -->
	<Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
	<!-- Please do not move/edit code below this line -->

	<PropertyGroup>
		<PackageId>Microsoft.Azure.Management.Network</PackageId>
		<Description>Provides management capabilities for Network services.</Description>
		<AssemblyName>Microsoft.Azure.Management.Network</AssemblyName>
		<Version>19.5.0-preview</Version>
		<PackageTags>Microsoft Azure Network management;Network;Network management;</PackageTags>    
		<PackageReleaseNotes>
		<![CDATA[
      - Added AddressPrefixes on Subnet properties
      - Included required properties for outbound rule 
      - Azure Firewall FQDN Tag added as a top level resource
      - Swagger Coverage/Completeness- for operation CheckNameAvailability
      - Azure Firewall NAT Rule Collection 
      - Added support for interface endpoints
      - Added linkedResourceType and hostedWorkloads to nic 
      - Added App Gateway Trusted root certificate changes 
      - Added ExpressRoute gateway resource
      - Added support for delegations on a subnet      
		]]></PackageReleaseNotes>
	</PropertyGroup>
	<PropertyGroup>
		<TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
	</PropertyGroup>

	<!-- Please do not move/edit code below this line -->
	<Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
	<!-- Please do not move/edit code above this line -->
</Project>
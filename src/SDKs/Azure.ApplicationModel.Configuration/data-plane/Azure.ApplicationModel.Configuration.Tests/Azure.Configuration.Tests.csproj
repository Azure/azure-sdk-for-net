    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <SignTestAssembly>true</SignTestAssembly>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="nunit" Version="3.10.1" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.10.0" />

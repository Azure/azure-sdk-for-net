<Project Sdk="Microsoft.NET.Sdk">
  <!-- Please do not move/edit code below this line -->
  <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
  <!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Azure.AlertsManagement</PackageId>
    <Description>Provides developers with libraries for the Alerts Management</Description>
    <Version>1.0.0</Version>
    <AssemblyName>Microsoft.Azure.AlertsManagement</AssemblyName>
    <PackageTags>alerts;notification;alertsmanagement;</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
			This is a public release of the Azure AlertsManagement SDK.
		  ]]>
    </PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
  </PropertyGroup>

  <!-- Please do not move/edit code below this line -->
  <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
  <!-- Please do not move/edit code above this line -->
</Project>
{
  "version": "1.0.0-beta3",
  "title": "Microsoft Azure Management Client Library",
  "description": "Provides management (Fluent) capabilities for Microsoft Azure.",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Microsoft Azure Management library.",
    "iconUrl": "http://go.microsoft.com/fwlink/?LinkID=288890",
    "tags": [ "Microsoft Azure Management Fluent", "REST HTTP client", "azureofficial", "windowsazureofficial" ],
    "projectUrl": "https://github.com/Azure/azure-sdk-for-net/tree/Fluent",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE",
    "requireLicenseAcceptance": true
  },

  "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../../tools/MSSharedLibKey.snk"
  },

  "dependencies": {
    "Microsoft.Azure.Management.Batch.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Compute.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Dns.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.KeyVault.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Network.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.ResourceManager.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Sql.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Storage.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.TrafficManager.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Cdn.Fluent": "[1.0.0-beta3, 2.0)",
    "Microsoft.Azure.Management.Redis.Fluent": "[1.0.0-beta3, 2.0)"
  },

  "frameworks": {
    "net45": {
      "dependencies": {
      }
    },
    "netstandard1.5": {
      "imports": [ "dnxcore50" ],
      "buildOptions": { "define": [ "PORTABLE" ] },
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1",
        "System.Diagnostics.Tools": "4.0.1",
        "System.Net.Http": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Threading.Tasks": "4.0.11"
      }
    },
    "netstandard1.1": {
      "imports": [ "dnxcore50" ],
      "buildOptions": { "define": [ "PORTABLE" ] },
      "dependencies": {
        "System.Runtime.Serialization.Primitives": "4.1.1"
      }
    }
  }
}

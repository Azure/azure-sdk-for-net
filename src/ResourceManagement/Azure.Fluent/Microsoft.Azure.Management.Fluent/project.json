{
  "version": "1.0.0",
  "title": "Microsoft Azure Management Client Library",
  "description": "Provides management (Fluent) capabilities for Microsoft Azure.",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Microsoft Azure Management library.",
    "iconUrl": "http://go.microsoft.com/fwlink/?LinkID=288890",
    "tags": [ "Microsoft Azure Management Fluent", "REST HTTP client", "azureofficial", "windowsazureofficial" ],
    "projectUrl": "https://github.com/Azure/azure-sdk-for-net/tree/Fluent",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE",
    "requireLicenseAcceptance": true
  },

  "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../../tools/MSSharedLibKey.snk", 
    "xmlDoc": true
  },

  "dependencies": {
    "Microsoft.Azure.Management.AppService.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Batch.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Cdn.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Compute.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Dns.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.KeyVault.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Network.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Redis.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.ResourceManager.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Sql.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.Storage.Fluent": "[1.0.0, 2.0)",
    "Microsoft.Azure.Management.TrafficManager.Fluent": "[1.0.0, 2.0)"
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NET45" ],
        "debugType": "portable"
      },
      "dependencies": {
      }
    },
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "NETSTANDARD15", "PORTABLE" ],
        "debugType": "portable"
      },
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NETSTANDARD11", "PORTABLE" ],
        "debugType": "portable"
      },
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    }
  }
}

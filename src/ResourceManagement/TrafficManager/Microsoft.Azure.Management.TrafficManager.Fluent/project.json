{
    "version": "1.0.0-beta3",
    "title": "Microsoft Azure Traffic Manager Management Library",
    "description": "Provides Traffic Manager management (Fluent) capabilities for Microsoft Azure.",
    "authors": [ "Microsoft" ],

    "packOptions": {
        "summary": "Microsoft Azure Management Traffic Manager Library.",
        "iconUrl": "http://go.microsoft.com/fwlink/?LinkID=288890",
        "tags": [ "Microsoft Azure Traffic Manager management", "trafficmanager", "network", "REST HTTP client", "azureofficial", "windowsazureofficial", "Fluent" ],
        "projectUrl": "https://github.com/Azure/azure-sdk-for-net/tree/Fluent",
        "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE",
        "requireLicenseAcceptance": true

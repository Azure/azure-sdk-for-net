                "Microsoft.NETCore.Platforms": "1.0.1",
                "System.Diagnostics.Tools": "4.0.1",
                "System.Net.Http": "4.1.0",
                "System.Runtime.Serialization.Primitives": "4.1.1",
                "System.Threading.Tasks": "4.0.11"
            }
        },
        "netstandard1.1": {
            "imports": [ "dnxcore50" ],
            "dependencies": {
                "System.Runtime.Serialization.Primitives": "4.1.1"

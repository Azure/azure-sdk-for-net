{
  "version": "1.1.0",
  "title": "Microsoft Azure Compute Management Library",
  "description": "Provides virtual machine, virtual machine extensions and availability set management (Fluent) capabilities for Microsoft Azure.",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Microsoft Azure Compute Management Libraryary.",
    "iconUrl": "http://go.microsoft.com/fwlink/?LinkID=288890",
    "tags": [ "Microsoft Azure Compute management", "virtual machine", "compute", "REST HTTP client", "azureofficial", "windowsazureofficial", "Fluent" ],
    "projectUrl": "https://github.com/Azure/azure-sdk-for-net/tree/Fluent",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE",
    "requireLicenseAcceptance": true
  },

  "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../../tools/MSSharedLibKey.snk", 
    "xmlDoc": true
  },

  "dependencies": {
    "Microsoft.Rest.ClientRuntime.Azure": "3.3.8",
    "Microsoft.Azure.Management.ResourceManager.Fluent": "1.1.0",
    "Microsoft.Azure.Management.Network.Fluent": "1.1.0",
    "Microsoft.Azure.Management.Storage.Fluent": "1.1.0"
  },

  "frameworks": {
    "net452": {
      "buildOptions": {
        "define": [ "NET45" ],
        "debugType": "portable"
      },
      "dependencies": {
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD13", "PORTABLE" ],
        "debugType": "portable"
      },
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    }
  }
}

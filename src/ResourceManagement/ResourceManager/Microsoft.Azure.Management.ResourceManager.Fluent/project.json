{
  "version": "1.0.0-beta4",
  "title": "Microsoft Azure Resource Management Library",
  "description": "Provides resource group and resource management (Fluent) capabilities for Microsoft Azure.",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Microsoft Azure Resource Management Library.",
    "iconUrl": "http://go.microsoft.com/fwlink/?LinkID=288890",
    "tags": [ "Microsoft Azure Resource Manager", "resource", "REST HTTP client", "azureofficial", "windowsazureofficial", "Fluent" ],
    "projectUrl": "https://github.com/Azure/azure-sdk-for-net/tree/Fluent",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE",
    "requireLicenseAcceptance": true
  },

  "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../../tools/MSSharedLibKey.snk"
  },

  "dependencies": {
    "Microsoft.Rest.ClientRuntime.Azure": "[3.3.4, 4.0)",
    "Microsoft.Rest.ClientRuntime.Azure.Authentication": "[2.2.9-preview, 3.0)"
  },

  "frameworks": {
    "net45": {
      "dependencies": {
      }
    },
    "netstandard1.5": {
      "buildOptions": { "define": [ "PORTABLE" ] },
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1",
        "System.Diagnostics.Tools": "4.0.1",
        "System.Net.Http": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Threading.Tasks": "4.0.11",
        "System.IO.FileSystem": "4.0.1"
      }
    },
    "netstandard1.1": {
      "buildOptions": { "define": [ "PORTABLE" ] },
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.IO.FileSystem": "4.0.1"
      }
    }
  }
}

{            
    name: "DA_WikipediaClickEvents",
    properties:
    {
        location: 
        {
            type: "AzureBlobLocation",
            blobPath: "wikidatagateway/wikisampledatain/",
            linkedServiceName: "LinkedService-WikipediaClickEvents"
        },
        availability: {
            frequency: "Hour",
            interval: 1,
            waitOnExternal:
            {
                retryInterval: "00:01:00",
                retryTimeout: "00:10:00",
                maximumRetry: 3
            }
        },
        policy: 
        {
            validation:
            {
                minimumSizeMB: 1.0
            }
        }
    }
}

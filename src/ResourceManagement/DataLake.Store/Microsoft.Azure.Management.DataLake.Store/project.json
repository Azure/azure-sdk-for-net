      "buildOptions": {
        "define": [ "NETSTANDARD11", "PORTABLE" ],
        "debugType": "portable"
      },
      "imports": ["dnxcore50"],
      "dependencies": {
        "NETStandard.Library": "1.6.1",
        "System.Threading.Thread": "4.3.0"

<Project Sdk="Microsoft.NET.Sdk">

    <!-- Please do not move/edit code below this line -->
    <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
    <!-- Please do not move/edit code below this line -->

    <PropertyGroup>
        <PackageId>Microsoft.AzureStack.Management.InfrastructureInsights.Admin</PackageId>
        <Description>Provides developers with libraries for the health admin under AzureStack to manage health alerts.</Description>
        <VersionPrefix>0.3.0-preview</VersionPrefix>
        <AssemblyName>Microsoft.AzureStack.Management.InfrastructureInsights.Admin</AssemblyName>
        <PackageTags>Microsoft Azure Stack;InfrastructureInsights;REST HTTP client;azureostackfficial;netcore451511</PackageTags>
        <PackageReleaseNotes>
            <![CDATA[
                Preview release of the InfrastructureInsights administrator SDK which allows administrators  
                1) View the health of their AzureStack stamp resources 
                2) View and manage alerts
                Repair API is supported in azurestack 1811+
            ]]>
        </PackageReleaseNotes>
    </PropertyGroup>

    <PropertyGroup>
        <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
    </PropertyGroup>

    <!-- Please do not move/edit code below this line -->
    <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
    <!-- Please do not move/edit code above this line -->
</Project>

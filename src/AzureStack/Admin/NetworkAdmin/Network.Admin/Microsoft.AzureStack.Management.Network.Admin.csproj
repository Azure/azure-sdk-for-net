<Project Sdk="Microsoft.NET.Sdk">

    <!-- Please do not move/edit code below this line -->
    <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
    <!-- Please do not move/edit code below this line -->

    <PropertyGroup>
        <PackageId>Microsoft.AzureStack.Management.Network.Admin</PackageId>
        <Description>Provides developers with libraries for the network admin under AzureStack to manage network.</Description>
        <VersionPrefix>0.1.0-preview</VersionPrefix>
        <AssemblyName>Microsoft.AzureStack.Management.Network.Admin</AssemblyName>
        <PackageTags>Microsoft Azure Stack;Network;REST HTTP client;azureostackfficial;netcore451511</PackageTags>
        <PackageReleaseNotes>
            <![CDATA[
                Preview release of the Network administrator SDK which allows  
                1) Management of network quotas  
                2) View allocated network resources such as public IP addresses, virtual networks, load balancers. 
                3) Provides a cmdlet which provides an administrator overview 
            ]]>
        </PackageReleaseNotes>
    </PropertyGroup>

    <PropertyGroup>
        <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
    </PropertyGroup>

    <!-- Please do not move/edit code below this line -->
    <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
    <!-- Please do not move/edit code above this line -->
</Project>

<Project Sdk="Microsoft.NET.Sdk">

    <!-- Please do not move/edit code below this line -->
    <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
    <!-- Please do not move/edit code below this line -->

    <PropertyGroup>
        <PackageId>Microsoft.AzureStack.Management.Update.Admin</PackageId>
        <Description>Provides developers with libraries for the Update admin under AzureStack to manage plans and stuff.</Description>
        <VersionPrefix>0.1.0-preview</VersionPrefix>
        <AssemblyName>Microsoft.AzureStack.Management.Update.Admin</AssemblyName>
        <PackageTags>Microsoft Azure Stack;Update;REST HTTP client;azureostackfficial;netcore451511</PackageTags>
        <PackageReleaseNotes>
            <![CDATA[
                Preview release of the AzureStack Update administrator SDK.  With this SDK administrators can 
                1) List and install available updates 
                2) Resume interrupted updates 
                3) View installed updates 
            ]]>
        </PackageReleaseNotes>
    </PropertyGroup>

    <PropertyGroup>
        <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
    </PropertyGroup>

    <!-- Please do not move/edit code below this line -->
    <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
    <!-- Please do not move/edit code above this line -->
</Project>

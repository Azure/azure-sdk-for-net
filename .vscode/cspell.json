{
  "version": "0.1",
  "language": "en_US",
  "languageId": "csharp",
  "dictionaries": ["powershell", "softwareTerms", "csharp"],
  "ignorePaths": [
    "**/SessionRecords/**",
    "**/packages/**",
    "artifacts/**/*",
    "*.exe",
    "*.dll",
    "*.nupkg",
    "*.bmp",
    "*.png",
    "*.gif",
    "*.jpg",
    "*.tiff",
    "*.wav",
    "*.pdf",
    "*.zip",
    "*.sha512",
    "*.ico",
    "*.pri",
    "*.trx",
    "*.binlog",
    "*.log",
    "*.sln",
    "*.user",
    ".vscode/cspell.json"
  ],
  // cspell is not case sensitive
  // Sort words alphabetically to make this list easier to use
  "words": [
    "apos",
    "azsdk",
    "blazor",
    "contoso",
    "deserializes",
    "diagnoser",
    "dtmf",
    "epsg",
    "expando",
    "finalizer",
    "finalizers",
    "ints",
    "lucene",
    "mgmt",
    "nunit",
    "odata",
    "otel",
    "overridden",
    "parallelization",
    "pkcs",
    "pstn",
    "pwsh",
    "referer",
    "renormalize",
    "retriable",
    "structs",
    "uints",
    "unformattable",
    "unhold",
    "uninstrumented",
    "westus",
    "xunit"
  ],
  "overrides": [
    {
      "filename": "**/sdk/formrecognizer/**/*.cs",
      "words": ["ZhHant"]
    },
    {
      "filename": "**/eng/pipelines/templates/jobs/ci.yml",
      "words": ["warnaserror"]
    }
  ],
  "allowCompoundWords": true
}

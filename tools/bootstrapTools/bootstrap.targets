<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<UsingTask TaskName="GetBuildTools" AssemblyFile="Microsoft.Azure.Build.BootstrapTasks.dll" />
	
  <Target Name="UpdateTools">
	<Message Text="Updating tools ...."/>
	<GetBuildTools LocalBranchRootDir="$(LibraryRoot)" RemoteRootDir="https://raw.githubusercontent.com/shahabhijeet/azure-sdk-for-net/addSep/" DebugTrace="$(DebugTrace)"/>
  </Target>
</Project>

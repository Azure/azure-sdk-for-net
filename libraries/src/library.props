  <PropertyGroup>
    <LibraryPropertiesProjectIncluded>true</LibraryPropertiesProjectIncluded>
    <OutputPath>$(BuildOutputFolder)\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">

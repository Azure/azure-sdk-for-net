    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <!--
  IsStandardWindowsAzureLibrary support
  -->
  <PropertyGroup>
    <WindowsAzureLibraryPrimaryConfiguration 
      Condition=" '$(WindowsAzureLibraryProfileSet)' == 'portable/net40' And '$(WindowsAzureLibraryPrimaryConfiguration)' == '' ">portable</WindowsAzureLibraryPrimaryConfiguration>
    <WindowsAzureLibrarySecondaryConfiguration 
      Condition=" '$(WindowsAzureLibraryProfileSet)' == 'portable/net40' And  '$(WindowsAzureLibrarySecondaryConfiguration)' == '' ">net40</WindowsAzureLibrarySecondaryConfiguration>
    <WindowsAzureLibraryPrimaryConfiguration 
      Condition=" '$(WindowsAzureLibraryProfileSet)' == 'net45/net40' And '$(WindowsAzureLibraryPrimaryConfiguration)' == '' ">net45</WindowsAzureLibraryPrimaryConfiguration>
    <WindowsAzureLibrarySecondaryConfiguration 
      Condition=" '$(WindowsAzureLibraryProfileSet)' == 'net45/net40' And  '$(WindowsAzureLibrarySecondaryConfiguration)' == '' ">net40</WindowsAzureLibrarySecondaryConfiguration>
  </PropertyGroup>
  <PropertyGroup>
    <WindowsAzureCurrentConfiguration 
      Condition=" '$(BuildSecondConfiguration)' != 'true' ">$(WindowsAzureLibraryPrimaryConfiguration)</WindowsAzureCurrentConfiguration>
    <WindowsAzureCurrentConfiguration 
      Condition=" '$(BuildSecondConfiguration)' == 'true' ">$(WindowsAzureLibrarySecondaryConfiguration)</WindowsAzureCurrentConfiguration>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(WindowsAzureCurrentConfiguration)' == 'portable' ">

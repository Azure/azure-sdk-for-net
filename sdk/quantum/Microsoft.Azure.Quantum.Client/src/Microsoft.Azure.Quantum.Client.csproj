<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageTags>Microsoft Azure Quantum Client;Microsoft Azure Quantum Client;Quantum Client</PackageTags>
    <Description>Use this package if you're developing a Quantum client.</Description>
    <AssemblyTitle>Microsoft Azure Quantum Client Library</AssemblyTitle>
    <TargetFrameworks>net452;netstandard1.4;net461;netstandard2.0</TargetFrameworks>
    <PackageReleaseNotes>See the Microsoft.Azure.Quantum.Client package for detailed release notes on the entire Quantum Client .NET SDK.</PackageReleaseNotes>
    <Version>10.1.0</Version>
  </PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Rest.ClientRuntime.Azure"/>
		<PackageReference Include="Microsoft.Rest.ClientRuntime"/>
		<PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" PrivateAssets="All" />
	</ItemGroup>
</Project>
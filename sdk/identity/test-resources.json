{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "baseName": {
            "type": "string",
            "defaultValue": "[resourceGroup().name]",
            "metadata": {
                "description": "The base resource name."
            }
        },
        "azureIdentityTestTenantId": {
            "type": "string",
            "metadata": {
                "description": "The tenant ID to which the application and resources belong."
            }
        },
        "azureIdentityTestUsername": {
            "type": "string",
            "metadata": {
                "description": "The test username used to run tests."
            }
        },
        "azureIdentityTestPassword": {
            "type": "string",
            "metadata": {
                "description": "The test password used to run tests."
            }
        }
    },
    "variables": {},
    "resources": [],
    "outputs": {
        "AZURE_IDENTITY_TEST_TENANTID": {
            "type": "string",
            "value": "[parameters('azureIdentityTestTenantId')]"
        },
        "AZURE_IDENTITY_TEST_USERNAME": {
            "type": "string",
            "value": "[parameters('azureIdentityTestUsername')]"
        },
        "AZURE_IDENTITY_TEST_PASSWORD": {
            "type": "string",
            "value": "[parameters('azureIdentityTestPassword')]"
        }
    }
}

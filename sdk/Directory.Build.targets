<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="CleanUnusedFiles" AfterTargets="Clean">
    <ItemGroup>
      <UnusedFiles Include="@(Compile)" Condition="'%(Compile.Identity)' == 'unusedfile.cs'" />
    </ItemGroup>
    <Delete Files="@(UnusedFiles)" />
  </Target>
</Project>

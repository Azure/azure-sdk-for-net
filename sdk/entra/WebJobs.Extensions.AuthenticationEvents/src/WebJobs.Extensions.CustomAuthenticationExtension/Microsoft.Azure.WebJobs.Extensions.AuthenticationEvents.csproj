<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
		<IsExtensionClientLibrary>true</IsExtensionClientLibrary>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Azure.WebJobs" />
		<PackageReference Include="Microsoft.AspNetCore.Http" />
		<PackageReference Include="Microsoft.IdentityModel.Protocols" />
		<PackageReference Include="Microsoft.IdentityModel.Protocols.OpenIdConnect" />
	</ItemGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<WarningLevel>5</WarningLevel>
		<DocumentationFile>WebJobs.Extensions.AuthenticationEvents.xml</DocumentationFile>
	</PropertyGroup>

	
	<ItemGroup>
		<Compile Remove="lib\**" />
		<Compile Remove="Requests\**" /> 
		<Compile Remove="Responses\**" />
		<EmbeddedResource Remove="lib\**" />
		<EmbeddedResource Remove="Requests\**" />
		<EmbeddedResource Remove="Responses\**" />
		<None Remove="lib\**" />
		<None Remove="Requests\**" />
		<None Remove="Responses\**" />
	</ItemGroup>

	<ItemGroup>
		<Compile Remove="HttpTriggerAttribute.cs" />
	</ItemGroup>

	<ItemGroup>
		<None Remove="Schema\TokenIssuanceStart\preview_10_01_2021\openapi.json" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Schema\TokenIssuanceStart\openapi.json" />
	  <EmbeddedResource Include="Schema\TokenIssuanceStart\preview_10_01_2021\openapi.json" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Schema\TokenIssuanceStart\preview_10_01_2021\requestSchema.json" />
		<EmbeddedResource Include="Schema\TokenIssuanceStart\preview_10_01_2021\responseSchema.json" />
		<EmbeddedResource Include="Schema\TokenIssuanceStart\requestSchema.json" />
		<EmbeddedResource Include="Schema\TokenIssuanceStart\responseSchema.json" />
		<EmbeddedResource Include="Templates\ActionableTemplate.json" />
		<EmbeddedResource Include="Templates\CloudEventActionableTemplate.json" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="AuthEventResource.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>AuthEventResource.resx</DependentUpon>
		</Compile>
		<Compile Update="AuthEventResourceAuthEventResource.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>AuthEventResourceAuthEventResource.resx</DependentUpon>
		</Compile>
		<Compile Update="Properties\Settings.Designer.cs">
		  <DesignTimeSharedInput>True</DesignTimeSharedInput>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>Settings.settings</DependentUpon>
		</Compile>
	</ItemGroup>


	<ItemGroup>
		<EmbeddedResource Update="AuthEventResource.resx">
		  <Generator>ResXFileCodeGenerator</Generator>
		  <LastGenOutput>AuthEventResource.Designer.cs</LastGenOutput>
		</EmbeddedResource>
		<EmbeddedResource Update="AuthEventResourceAuthEventResource.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>AuthEventResourceAuthEventResource.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>


	<ItemGroup>
		<None Update=".gitignore">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="Properties\Settings.settings">
		  <Generator>SettingsSingleFileGenerator</Generator>
		  <LastGenOutput>Settings.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>


	<ItemGroup>
		<Folder Include="Help\" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\README.md" Pack="true" PackagePath="" />
	</ItemGroup>
</Project>

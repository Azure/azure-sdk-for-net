<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>This is the Azure.Messaging.EventGrid.Namespaces client library for developing .NET applications with rich experience.</Description>
    <AssemblyTitle>Azure SDK Code Generation Azure.Messaging.EventGrid.Namespaces for Azure Data Plane</AssemblyTitle>
    <Version>1.0.0-beta.1</Version>
    <PackageTags>Azure.Messaging.EventGrid.Namespaces</PackageTags>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <IncludeOperationsSharedSource>true</IncludeOperationsSharedSource>
    <DisableEnhancedAnalysis>true</DisableEnhancedAnalysis>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(AzureCoreSharedSources)AzureResourceProviderNamespaceAttribute.cs" LinkBase="Shared/Core" />
    <Compile Include="..\..\..\core\Azure.Core\src\Shared\AzureKeyCredentialPolicy.cs">
      <Link>Shared\Core\AzureKeyCredentialPolicy.cs</Link>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Core" />
    <PackageReference Include="System.Text.Json" />
  </ItemGroup>

</Project>

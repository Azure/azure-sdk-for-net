<Project Sdk="Microsoft.NET.Sdk"> 
  <PropertyGroup>
    <VersionPrefix>3.8.0-preview</VersionPrefix>
    <Description>Provides Microsoft Azure Graph RBAC access.</Description>
    <AssemblyTitle>Microsoft Azure Graph RBAC Library</AssemblyTitle>
    <PackageTags>Microsoft AutoRest Management REST;netcore451511</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
		  Regenerated .net sdk from the latest version of GraphRbac rest specification.
      ]]>
    </PackageReleaseNotes>
  </PropertyGroup>

  <!-- Disable StyleCop -->
  <PropertyGroup>
    <CodeAnalysisRuleSet>$(RepoEngPath)\CodeAnalysisDisabled.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <AzureApiTag>GraphRbacManagementClient_1.6;</AzureApiTag>
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>
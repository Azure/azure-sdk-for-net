<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.0-beta.1</Version>
    <Description>This package extends the Microsoft.Azure.WebJobs library with Table triggers using Microsoft Azure Storage.</Description>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <AssemblyName>Microsoft.Azure.WebJobs.Extensions.Table</AssemblyName>
    <RootNamespace>Microsoft.Azure.WebJobs.Extensions.Table</RootNamespace>
    <NoWarn>$(NoWarn);CS1606;CS0618;AZC0001;CS1591;SA1636;SA1028;CA1200</NoWarn>
  </PropertyGroup>  

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos.Table" VersionOverride="1.0.7" />
    <PackageReference Include="Microsoft.Azure.Storage.Blob" VersionOverride="11.1.7" />
    <PackageReference Include="Microsoft.Azure.Storage.Queue" VersionOverride="11.1.7" />
    <PackageReference Include="Microsoft.Azure.WebJobs" VersionOverride="3.0.22" />
  </ItemGroup>

</Project>

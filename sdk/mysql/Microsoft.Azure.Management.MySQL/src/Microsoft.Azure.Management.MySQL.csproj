<Project Sdk="Microsoft.NET.Sdk">
	<!-- Please do not move/edit code below this line -->
	<Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
	<!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Azure.Management.MySQL</PackageId>
    <Description>Provides developers with libraries for the Azure Database for MySQL under Azure Resource manager to deploy an instance of MySQL. Create, Update, Delete, Restore, and more.</Description>
    <Version>0.1-preview</Version>
    <AssemblyName>Microsoft.Azure.Management.MySQL</AssemblyName>
    <PackageTags>MySQL;Azure Database for MySQL;</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
        This is a initial public release of the Azure Database for MySQL SDK. This release includes management libraries to 
       1. Create and manage MySQL Servers
       2. Create and manage MySQL Flexible Servers
       For complete list of API's and method please refer to the documentation.
      ]]>
    </PackageReleaseNotes>
  </PropertyGroup>
	<PropertyGroup>
		<TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
	</PropertyGroup>

	<!-- Please do not move/edit code below this line -->
	<Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
	<!-- Please do not move/edit code above this line -->
</Project>
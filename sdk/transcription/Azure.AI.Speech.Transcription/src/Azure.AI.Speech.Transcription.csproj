<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>This is the Azure.AI.Speech.Transcription client library for developing .NET applications with rich experience.</Description>
    <AssemblyTitle>SDK Code Generation Azure.AI.Speech.Transcription</AssemblyTitle>
    <Version>1.0.0-beta.1</Version>
    <PackageTags>Azure.AI.Speech.Transcription</PackageTags>
    <TargetFrameworks>netstandard2.0;net8.0;net9.0;net10.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <!-- Disable AutoRest dependency since this SDK uses @typespec/http-client-csharp -->
    <IncludeAutorestDependency>false</IncludeAutorestDependency>
    <!-- Suppress warnings for generated code -->
    <NoWarn>$(NoWarn);CS1591;AZC0007;AZC0012;AZC0015;AZC0034;AZC0102;SCM0005</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ClientModel" />
  </ItemGroup>
</Project>

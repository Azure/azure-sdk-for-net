<Project Sdk="Microsoft.NET.Sdk">
  <!-- Please do not move/edit code below this line -->
  <Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
  <!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Azure.Management.Kubernetes</PackageId>
    <Description>
      Provides developers with libraries for the hybrid kubernetes platform under Azure Resource manager to deploy Connected Clusters resource. Note: This client library is for Connected Clusters under Azure Resource Manager.
    </Description>
    <Version>1.0.0-beta.1</Version>
    <AssemblyName>Microsoft.Azure.Management.Kubernetes</AssemblyName>
    <PackageTags>management;kubernetes;</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
        This is a public release of the Azure Arc for Kubernetes SDK.
      ]]>
    </PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
  </PropertyGroup>

  <!-- Please do not move/edit code below this line -->
  <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
  <!-- Please do not move/edit code above this line -->
</Project>

{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Test (MacOS)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "/usr/local/bin/dotnet",
            "args": [ "test", "-f", "netcoreapp1.0" ],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}
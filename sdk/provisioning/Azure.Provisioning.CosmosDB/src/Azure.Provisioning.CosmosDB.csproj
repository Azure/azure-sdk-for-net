<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Azure.Provisioning.CosmosDB simplifies declarative resource provisioning in .NET.</Description>
    <Version>1.1.0-beta.2</Version>
    <!--The ApiCompatVersion is managed automatically and should not generally be modified manually.-->
    <ApiCompatVersion>1.0.0</ApiCompatVersion>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <LangVersion>12</LangVersion>

    <!-- Disable warning CS1591: Missing XML comment for publicly visible type or member -->
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <IncludeAutorestDependency>true</IncludeAutorestDependency>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Provisioning" />
  </ItemGroup>

  <!-- TODO: Consider adding DataPlane dependencies here like:
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos" />
  </ItemGroup>
  -->
</Project>

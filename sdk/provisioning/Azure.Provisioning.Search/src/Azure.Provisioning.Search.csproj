<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Azure.Provisioning.Search simplifies declarative resource provisioning in .NET for Azure AI Search.</Description>
    <Version>0.1.0-beta.2</Version>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <Nullable>enable</Nullable>
    <NoWarn>$(NoWarn);AZC0001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.ResourceManager.Search" />
  </ItemGroup>

</Project>

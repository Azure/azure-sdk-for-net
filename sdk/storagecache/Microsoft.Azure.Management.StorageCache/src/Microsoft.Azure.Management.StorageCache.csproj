<Project Sdk="Microsoft.NET.Sdk">
	<!-- Please do not move/edit code below this line -->
	<Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
	<!-- Please do not move/edit code below this line -->

	<PropertyGroup>
		<PackageId>Microsoft.Azure.Management.StorageCache</PackageId>
		<Description>Microsoft Azure StorageCache Management Library</Description>
		<AssemblyName>Microsoft.Azure.Management.StorageCache</AssemblyName>
    <AssemblyTitle>Microsoft Azure HPC StorageCache Management</AssemblyTitle>
		<Version>1.1.0</Version>
		<PackageTags>Microsoft Azure Storagecache management;StorageCache;StorageCache management;StorageCache_2020-03-01;</PackageTags>
		<PackageReleaseNotes>
			<![CDATA[
      Microsoft Azure HPC Storage Cache management .NET SDK.

			Azure HPC Storage Cache provides scalable caching service for NAS clients,
			serving data from either NFSv3 or Blob at-rest storage (referred to as \"Storage Targets\").
			These operations allow you to manage caches.

      Version 1.1.0 relates to API vesrion 2020-03-01.
      This release contatins changes related to,
      . Cache managed keys &
      . Disk encryption
 			]]>
		</PackageReleaseNotes>

	</PropertyGroup>
	<PropertyGroup>
		<TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
		<ApplicationIcon />
		<OutputType>Library</OutputType>
		<StartupObject />
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net452|AnyCPU'">
	  <PlatformTarget>AnyCPU</PlatformTarget>
	  <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
	</PropertyGroup>

	<!-- Please do not move/edit code below this line -->
	<Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />

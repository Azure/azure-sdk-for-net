    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Generate Blobs",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "runtimeExecutable": "autorest",
            "runtimeArgs": [
                "${workspaceFolder}/../../Swagger/Blobs/readme.md",
                "--verbose",
                "--use=${workspaceFolder}"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Blobs",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "program": "${workspaceFolder}/dist/index.js",
            "args": [
                "--debug",
                "${workspaceFolder}/../../Swagger/Blobs/blob.json",
                "${workspaceFolder}/../../Azure.Storage.Blobs/src"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Generate Queues",
            "preLaunchTask": "tsc: build - tsconfig.json",

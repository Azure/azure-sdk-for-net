<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <Version>5.1.2</Version>
    <RunApiCompat>false</RunApiCompat>
    <Description>This extension adds bindings for Storage</Description>
    <!-- This package is a metapackage. The flag below makes sure it doesn't include any DLL-->
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <!-- Remove once https://github.com/NuGet/Home/issues/8583 is fixed -->
    <NoWarn>$(NoWarn);NU5128</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions.Storage.Blobs" />
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions.Storage.Queues" />
  </ItemGroup>
</Project>

    <Compile Include="$(AzureCoreSharedSources)TaskExtensions.cs" LinkBase="SharedCore" />
    <Compile Include="$(AzureCoreSharedSources)Argument.cs" LinkBase="SharedCore" />
  </ItemGroup>
  <ItemGroup>

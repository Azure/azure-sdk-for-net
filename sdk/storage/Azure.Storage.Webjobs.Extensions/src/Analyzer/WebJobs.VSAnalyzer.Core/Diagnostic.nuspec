<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Microsoft.Azure.WebJobs.Analyzer</id>
    <version>2.0.0.0</version>
    <title>Azure Functions Analyzer</title>
    <authors>Microsoft</authors>
    <owners>Microsoft</owners>
    <licenseUrl>http://www.microsoft.com/web/webpi/eula/aspnetcomponent_rtw_enu.htm</licenseUrl>
    <projectUrl>http://go.microsoft.com/fwlink/?LinkID=320972</projectUrl>    
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Analyzer for Azure Functions and WebJobs. (Functions v2 only)</description>
    <releaseNotes>Summary of changes made in this release of the package.</releaseNotes>
    <copyright>Copyright Microsoft 2017</copyright>
    <tags>MyAnalyzer, analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>
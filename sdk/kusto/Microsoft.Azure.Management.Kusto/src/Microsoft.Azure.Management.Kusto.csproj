<Project Sdk="Microsoft.NET.Sdk">
  <!-- Please do not move/edit code below this line -->
  <Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
  <!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Azure.Management.Kusto</PackageId>
    <Version>6.0.0</Version>
    <AssemblyName>Microsoft.Azure.Management.Kusto</AssemblyName>
    <PackageTags>management;kusto</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[
        The C# client provides management capabilities for Azure Data Explorer (Kusto) entities. The following capabilities are included with this release:
        - Add EnableDoubleEncryption property to cluster.
        - Add IgnoreFirstRecord and BlobStorageEventType properties to Event Grid data connection.
        - New data formats for data connections: 'APACHEAVRO', 'W3CLOGFILE'
      ]]>
  </PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
  </PropertyGroup>

  <!-- Please do not move/edit code below this line -->
  <Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
  <!-- Please do not move/edit code above this line -->
</Project>
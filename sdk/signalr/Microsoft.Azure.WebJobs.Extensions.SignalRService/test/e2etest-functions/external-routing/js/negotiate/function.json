{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "signalRNegotiation",
      "direction": "in",
      "name": "negoCtx",
      "userId": "{query.userId}",
      "hubName": "%hub%"
    }
  ]
}
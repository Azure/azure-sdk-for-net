<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>This is the Authoring client library for developing .NET applications with rich experience.</Description>
    <AssemblyTitle>Azure SDK Code Generation Authoring for Azure Data Plane</AssemblyTitle>
    <Version>2.0.0-beta.1</Version>
    <PackageTags>Azure Authoring</PackageTags>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <IncludeOperationsSharedSource>true</IncludeOperationsSharedSource>
    <IncludeAutorestDependency>true</IncludeAutorestDependency>
    <AssemblyName>Azure.AI.Language.QuestionAnswering</AssemblyName>
    <PackageId>Azure.AI.Language.QuestionAnswering</PackageId>
    <NoWarn>$(NoWarn);AZC0034;AZC0012</NoWarn>
  </PropertyGroup>

  <!-- Shared source from Azure.Core -->
  <ItemGroup>
    <Compile Include="$(AzureCoreSharedSources)ArrayBufferWriter.cs" LinkBase="Shared" />
    <Compile Include="$(AzureCoreSharedSources)AzureResourceProviderNamespaceAttribute.cs" LinkBase="Shared" />
    <Compile Include="$(AzureCoreSharedSources)AzureKeyCredentialPolicy.cs" LinkBase="Shared" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Core" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Azure.AI.Language.QuestionAnswering\src\Azure.AI.Language.QuestionAnswering.csproj" />
  </ItemGroup>

</Project>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.0-beta.8</Version>
    <PackageId>Azure.ResourceManager</PackageId>
    <Description>
      Azure management SDK for Azure resources.
      This is a beta preview vesion.  This version uses a next-generation code generator that introduces important breaking changes, but also new features (such as intuitive authentication, custom HTTP pipeline, distributed tracing and much more).
    </Description>
    <PackageTags>azure;management;resource</PackageTags>
    <UseDefaultNamespaceAndOutputFolder>false</UseDefaultNamespaceAndOutputFolder>
    <NoWarn>CA1801;AZC0012;CA1062;CA2225;CA1308;CA1066;CA1716;CA1031</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(AzureCoreSharedSources)ForwardsClientCallsAttribute.cs" LinkBase="Shared\Core" />
    <Compile Include="$(AzureCoreSharedSources)HashCodeBuilder.cs" LinkBase="Shared\Core" />
  </ItemGroup>

</Project>

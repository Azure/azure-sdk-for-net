<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>1.0.0-beta.1</Version>
    <PackageId>Azure.ResourceManager.Authorization</PackageId>
    <Description>Azure Resource Manager client SDK for Azure resource provider Microsoft.Authorization</Description>
    <PackageTags>azure;management;arm;resource manager;authorization</PackageTags>
    <ExcludeMgmtCoreShared>true</ExcludeMgmtCoreShared>
    <IncludeGeneratorSharedCode>true</IncludeGeneratorSharedCode>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <PropertyGroup>
    <!-- TODO: As part of release process, this PropertyGroup can be removed once RP is approved for namespace -->
    <NoWarn>$(NoWarn);AZC0001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.AutoRest.CSharp" VersionOverride="$(MgmtAutorestVersion)" PrivateAssets="All" />
  </ItemGroup>
</Project>

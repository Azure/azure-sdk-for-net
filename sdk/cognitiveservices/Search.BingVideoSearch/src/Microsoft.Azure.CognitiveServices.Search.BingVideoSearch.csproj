<Project Sdk="Microsoft.NET.Sdk"> 
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services Video Search Client Library</AssemblyTitle>
    <Description>This client library provides access to the Microsoft Cognitive Services Video Search API.</Description>
    <Version>2.1.0-preview.1</Version>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;VideoSearch API;BingVideoSearch API;REST HTTPclient;VideoSearchSDK;BingVideoSearchSDK;netcore451511</PackageTags>
    <PackageReleaseNotes>
    <![CDATA[
    Changes in this release:
    1. Support overriding the default endpoint.
    2. Change client class name to `VideoSearchClient`.
    ]]>
	</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AzureApiTag />
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services Language LUIS Runtime</AssemblyTitle>
    <Description>Provides API functions for consuming the Microsoft Azure Cognitive Services LUIS Runtime API.</Description>
    <Version>3.1.0-preview.1</Version>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;LUIS;Language Understanding Intelligent Service;Language Understanding;REST HTTP client</PackageTags>
    <PackageReleaseNotes>
    <![CDATA[
    This is a public release of the Cognitive Services LUIS Runtime SDK.

    Changes in this release:
      - Support the runtime v3.0 APIs.
    ]]></PackageReleaseNotes>
    <IncludeAutorestDependency>true</IncludeAutorestDependency>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AzureApiTag />
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>

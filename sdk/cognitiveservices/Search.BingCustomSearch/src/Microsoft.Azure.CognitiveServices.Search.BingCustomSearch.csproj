<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services Custom Search Client Library</AssemblyTitle>
    <Description>This client library provides access to the Microsoft Cognitive Services Custom Search API.</Description>
    <Version>2.1.0-preview.1</Version>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;CustomSearch API;BingCustomSearch API;REST HTTPclient;CustomSearchSDK;BingCustomSearchSDK;netcore451511</PackageTags>
    <PackageReleaseNotes>
    <![CDATA[
    Changes in this release:
    1. Support overriding the default endpoint.
    ]]>
    </PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  
  <PropertyGroup>
    <AzureApiTag />
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>

<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services SpellCheck Client Library</AssemblyTitle>
    <Description>Provides API functions for consuming the Microsoft Azure Cognitive Services SpellCheck API.</Description>
    <VersionPrefix>4.0.0</VersionPrefix>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;SpellCheck API;REST HTTPclient;SpellCheckSDK;netcore451511</PackageTags>
    <PackageReleaseNotes>
    <![CDATA[
    Changes in this release:
    1. Support overriding the default endpoint.
    ]]>
    </PackageReleaseNotes>
  </PropertyGroup>

  <!-- Disable StyleCop -->
  <PropertyGroup>
    <CodeAnalysisRuleSet>$(RepoEngPath)\CodeAnalysisDisabled.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
</Project>

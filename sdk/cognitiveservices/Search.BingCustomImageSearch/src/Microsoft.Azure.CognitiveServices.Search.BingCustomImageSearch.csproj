<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services Custom Image Search Client Library</AssemblyTitle>
    <Description>This client library provides access to the Microsoft Cognitive Services Custom Image Search API.</Description>
    <VersionPrefix>2.0.0</VersionPrefix>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;CustomImageSearch API;BingImageCustomSearch API;REST HTTPclient;CustomImageSearchSDK;BingCustomImageSearchSDK;netcore451511</PackageTags>
	<PackageReleaseNotes>
    <![CDATA[
    Changes in this release:
    1. Support overriding the default endpoint.
    ]]>
	</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <AzureApiTag />
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>

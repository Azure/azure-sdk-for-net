<Project Sdk="Microsoft.NET.Sdk"> 
  <PropertyGroup>
    <AssemblyTitle>Microsoft Azure Cognitive Services Image Search Client Library</AssemblyTitle>
    <Description>This client library provides access to the Microsoft Cognitive Services Image Search API.</Description>
    <Version>2.1.0-preview.1</Version>
    <PackageTags>Microsoft Cognitive Services;Cognitive Services;Cognitive Services SDK;ImageSearch API;BingImageSearch API;REST HTTPclient;ImageSearchSDK;BingImageSearchSDK;netcore451511</PackageTags>
    <PackageReleaseNotes>
    <![CDATA[
    Changes in this release:
    1. Support overriding the default endpoint.
    2. Change client class name to `ImageSearchClient`.
    ]]>
	</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <AzureApiTag />
    <PackageTags>$(PackageTags);$(CommonTags);$(AzureApiTag);</PackageTags>
  </PropertyGroup>
</Project>
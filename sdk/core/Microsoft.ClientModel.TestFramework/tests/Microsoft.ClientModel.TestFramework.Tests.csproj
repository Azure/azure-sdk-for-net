<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="EndToEndTests\LibraryClient\LibraryClientOptions.cs" />
  </ItemGroup>

  <!-- Reference the Client Library -->
  <ItemGroup>
    <ProjectReference Include="..\src\Microsoft.ClientModel.TestFramework.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NUnit3TestAdapter" VersionOverride="5.1.0" />
    <PackageReference Include="NUnit" VersionOverride="4.4.0" />
    <PackageReference Include="Moq" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" VersionOverride="17.13.0" />
  </ItemGroup>

  <ItemGroup>
    <!--
      Explicit references to pull in patched versions of ASP.NET Core packages
    -->
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel.Core" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel.Transport.Sockets" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>SourcePath</_Parameter1>
      <_Parameter2>$(MSBuildProjectDirectory)</_Parameter2>
    </AssemblyAttribute>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="EndToEndTests\SessionRecords\" />
  </ItemGroup>
</Project>
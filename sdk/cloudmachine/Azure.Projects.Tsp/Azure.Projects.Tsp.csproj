<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Azure.Projects.Tsp is an ASP.NET Core extensions for Azure.Projects</Description>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <Version>1.0.0-beta.1</Version>
    <TargetFrameworks>$(RequiredRunnableTargetFrameworks)</TargetFrameworks>
    <RequiredTargetFrameworks>$(RequiredRunnableTargetFrameworks)</RequiredTargetFrameworks>

<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    Force the sample files to see the central properties.  This is safe since they
    use project references and, therefore, assume they are building in the context
    of the Azure SDK repository only.
  -->
  <PropertyGroup>
    <ImportRepoCommonSettings>true</ImportRepoCommonSettings>
    <IsShippingLibrary>false</IsShippingLibrary>
    <IsSamplesProject>true</IsSamplesProject>
  </PropertyGroup>

  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />
</Project>

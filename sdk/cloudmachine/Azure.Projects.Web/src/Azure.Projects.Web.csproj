<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Azure.Projects.Web is an ASP.NET Core extensions for Azure.Projects</Description>
    <Version>1.0.0-beta.2</Version>
    <TargetFrameworks>$(RequiredRunnableTargetFrameworks)</TargetFrameworks>
    <RequiredTargetFrameworks>$(RequiredRunnableTargetFrameworks)</RequiredTargetFrameworks>
    <SupportsNetStandard20>false</SupportsNetStandard20>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="Properties\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Memory.Data" />
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Azure.Projects\src\Azure.Projects.csproj" />
  </ItemGroup>
</Project>

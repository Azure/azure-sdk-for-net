<Project Sdk="Microsoft.NET.Sdk">
  <!-- Please do not move/edit code below this line -->
  <Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
  <!-- Please do not move/edit code below this line -->

  <PropertyGroup>
    <PackageId>Microsoft.Orbital</PackageId>
    <Description>Provides management capabilities for Azure Orbital.</Description>
    <Version>1.0.0-beta.1</Version>
    <AssemblyName>Microsoft.Orbital</AssemblyName>
    <PackageTags>Microsoft Azure Orbital; Orbital; Azure Orbital</PackageTags>
    <PackageReleaseNotes>
      <![CDATA[SDK for Azure Orbital, which allows you to:
 - create/read/update/delete Spacecrafts.
 - create/read/update/delete Contact Profiles.
 - create/read/update/delete Contacts.
]]>
    </PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
  </PropertyGroup>

  <!-- Please do not move/edit code below this line -->

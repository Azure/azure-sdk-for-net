<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>This is the Npgsql client library for developing .NET applications with rich experience.</Description>
    <AssemblyTitle>Azure SDK Code Generation Npgsql for Azure Data Plane</AssemblyTitle>
    <Version>1.0.3-beta.20</Version>
    <PackageTags>Azure Npgsql</PackageTags>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
		<IsClientLibrary>true</IsClientLibrary>
		<IsExtensionClientLibrary>true</IsExtensionClientLibrary>
		<NoWarn>$(NoWarn);AZC0001</NoWarn>
  </PropertyGroup>

	<ItemGroup>
		<Compile
			Include="..\..\Microsoft.Azure.Data.Extensions.Common\src\TokenCredentialBaseAuthenticationProvider.cs"
			Link="TokenCredentialBaseAuthenticationProvider.cs" />
	</ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Core" />
		<PackageReference Include="Npgsql" />
  </ItemGroup>

</Project>
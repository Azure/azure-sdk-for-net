<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>1.0.0-preview.2</Version>
    <PackageId>Azure.ResourceManager.Insights</PackageId>
    <Description>Azure Resource Manager client SDK for Azure resource provider Microsoft.Insights</Description>
    <PackageTags>azure;management;arm;resource manager;insights</PackageTags>
    <IsMgmtSubLibrary>false</IsMgmtSubLibrary>
  </PropertyGroup>

  <PropertyGroup>
    <ExcludeMgmtCoreShared>true</ExcludeMgmtCoreShared>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.AutoRest.CSharp" VersionOverride="$(MgmtAutorestVersion)" PrivateAssets="All" />
  </ItemGroup>
</Project>

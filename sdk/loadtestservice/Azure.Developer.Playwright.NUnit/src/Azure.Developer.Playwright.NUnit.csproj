<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageTags>Azure, Cloud, Playwright, Playwright Workspaces, Playwright Testing</PackageTags>
    <Description>
      Package to integrate your Playwright test suite with Azure Playwright.
    </Description>
    <Version>1.1.0-beta.1</Version>
    <!--The ApiCompatVersion is managed automatically and should not generally be modified manually.-->
    <ApiCompatVersion>1.0.0</ApiCompatVersion>
    <IsClientLibrary>true</IsClientLibrary>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Azure.Developer.Playwright\src\Azure.Developer.Playwright.csproj" />
    <PackageReference Include="NUnit" />
    <PackageReference Include="Azure.Core" />
  </ItemGroup>
</Project>
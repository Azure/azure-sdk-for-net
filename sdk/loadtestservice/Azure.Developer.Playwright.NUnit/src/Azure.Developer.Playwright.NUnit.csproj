<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageTags>Azure, Cloud, Playwright, Playwright Service, Playwright Testing</PackageTags>
    <Description>
      Package to integrate your Playwright test suite with Azure Playwright.
    </Description>
    <Version>1.0.0-beta.1</Version>
    <IsClientLibrary>true</IsClientLibrary>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Azure.Developer.Playwright\src\Azure.Developer.Playwright.csproj" />
    <PackageReference Include="NUnit" />
    <PackageReference Include="Azure.Core" />
  </ItemGroup>
</Project>
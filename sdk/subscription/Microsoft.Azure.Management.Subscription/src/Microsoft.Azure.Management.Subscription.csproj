<Project Sdk="Microsoft.NET.Sdk">
	<!-- Please do not move/edit code below this line -->
	<Import Project="$(RepoEngPath)/mgmt/AzSdk.reference.props" />
	<!-- Please do not move/edit code below this line -->

	<PropertyGroup>
		<PackageId>Microsoft.Azure.Management.Subscription</PackageId>
		<Description>Provides subscription management capabilities for Microsoft Azure.</Description>
		<AssemblyName>Microsoft.Azure.Management.Subscription</AssemblyName>
		<Version>2.0.0</Version>
		<PackageTags>Microsoft Alias Subscription;</PackageTags>
		<PackageReleaseNotes>
            <![CDATA[
                Breaking changes:
                * This is the new release of Microsoft.Subscription
                * SDK structure has been updated to make it more consistent and user-friendly:				  
				  1. Gets all available geo-locations ability is removed.
				  2. Gets details about a specified subscription ability is removed.
				  3. Gets all subscriptions for a tenant ability is removed.
				  4. Post subscription ownership API is added.
				  5. Get subscription ownership status API is added.
				  6. Put Subscription tenant policy for user's tenant API is added.
				  7. Get the subscription tenant policy for the user's tenant API is added.
				  8. Get all subscription tenant policy for the user's tenant API is added.
				  9. Get Billing Account Policy API is added.
            ]]>
     </PackageReleaseNotes>
	</PropertyGroup>
	<PropertyGroup>
		<TargetFrameworks>$(SdkTargetFx)</TargetFrameworks>
	</PropertyGroup>

	<!-- Please do not move/edit code below this line -->
	<Import Condition=" Exists('$([MSBuild]::GetPathOfFileAbove(AzSdk.RP.props))') " Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.RP.props'))" />
	<!-- Please do not move/edit code above this line -->
</Project>
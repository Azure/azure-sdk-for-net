{
  "$schema": "https://raw.githubusercontent.com/Azure/azure-sdk-tools/main/tools/spec-gen-sdk/src/types/SwaggerToSdkConfigSchema.json",
  "generateOptions": {
    "generateScript": {
      "path": "pwsh ./eng/scripts/Invoke-GenerateAndBuildV2.ps1",
      "logPrefix": ".Net",
      "stderr": {
        "showInComment": ".*(\\[ERROR\\]|ExternalError).*",
        "scriptError": "\\[ERROR\\]"
      },
      "stdout": {
        "showInComment": ".*(Start to|\\[ERROR\\]|: error|(?!0\\b)\\d+ Error\\(s\\)).*",
        "scriptError": "\\[ERROR\\]"
      }
    }
  },
  "initOptions": {
    "initScript": {
      "path": "pwsh ./eng/scripts/Automation-Sdk-Init.ps1",
      "logPrefix": ".Net"
    }
  },
  "packageOptions": {
    "packageFolderFromFileSearch": false,
    "buildScript": {
      "command": "dotnet build {packagePath}"
    }
  }
}
